{
    "name": "teamzac/laravel-tailnertiavue-preset",
    "description": "A Laravel preset for TailwindCSS/Inertiajs/Vue",
    "keywords": [
        "teamzac",
        "laravel",
        "tailwind-css",
        "inertia.js",
        "vue.js"
    ],
    "homepage": "https://github.com/teamzac/laravel-tailnertiavue-preset",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.8.0 || ^6.0",
        "inertiajs/inertia-laravel": "^0.1",
        "tightenco/ziggy": "^0.7.1"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TeamZac\\LaravelTailnertiavuePreset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamZac\\LaravelTailnertiavuePreset\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamZac\\LaravelTailnertiavuePreset\\TailnertiavuePresetServiceProvider"
            ]
        }
    }
}
